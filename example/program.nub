import c;

global func main(argc: i64, argv: i64) {
    printf("args: %d, starts at %p\n", argc, argv);

    a = 128;
    b = 32768;
    c = 2147483648;
    d = 9223372036850000000;

    x = test(a, b, c, d);

    printf("%d\n", x);
}

func test(a: i8, b: i16, c: i32, d: i64): i64 {
    printf("a: %d, b: %d, c: %d, d: %d\n", a, b, c, d);
    return 12;
}